<ul class="nav nav-pills">
  <li role="presentation"><%= link_to 'Approved', admin_photos_path(state: 'approved') %></li>
  <li role="presentation"><%= link_to 'Denied', admin_photos_path(state: 'denied') %></li>
  <li role="presentation"><%= link_to 'Pending', admin_photos_path(state: 'pending') %></li>
</ul>

<% @photos.each do |photo| %>

<div class="row">
  <div class="col-xs-6 col-md-3">
    <a href="<%= edit_admin_photo_path(photo) %>">
      <img src="<%= photo.image_url %>" width="200px" height="200px">
    </a>
  </div>
</div>
<% end %>